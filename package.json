{
  "name": "shopify-base-theme",
  "version": "1.5.8",
  "description": "A base shopify theme for custom theme development at Ruckus.",
  "main": "webpack.config.js",
  "scripts": {
    "start": "brrl -w",
    "build": "brrl -b -e production",
    "build-dev": "brrl -b -e development",
    "dependencies": "brrl-install",
    "development": "brrl -d -e development",
    "local-dev": "brrl -w -e development",
    "staging": "brrl -d -e staging",
    "production": "brrl -d -e production",
    "create-module": "sh scripts/create_module.sh",
    "create-mr": "sh scripts/create-merge-request.sh",
    "fix-css": "stylelint --fix 'src/css/base/*.css' && stylelint --fix 'src/modules/**/*.css'",
    "fix-js": "eslint --fix $PWD/'src/modules/**/*.js'",
    "fix-all": "eslint --fix $PWD/'src/modules/**/*.js' && stylelint --fix 'src/css/base/*.css' && stylelint --fix 'src/modules/**/*.css'",
    "test-css": "stylelint 'src/css/base/*.css' && stylelint 'src/modules/**/*.css'",
    "test-js": "eslint $PWD/'src/modules/**/*.js'",
    "test": "npm run test-js && npm run test-css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:ruckusco/shopify-base-theme.git"
  },
  "author": "Ruckus",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/ruckusco/shopify-base-theme/issues"
  },
  "homepage": "https://gitlab.com/ruckusco/shopify-base-theme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@barrelny/cli": "latest",
    "autoprefixer": "^7.2.6",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "css-loader": "^0.28.11",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^1.1.11",
    "globby": "^5.0.0",
    "gulp": "^3.9.1",
    "postcss": "^7.0.27",
